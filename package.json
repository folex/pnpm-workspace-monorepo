{
  "name": "monorepo",
  "private": true,
  "author": "Innei",
  "license": "MIT",
  "scripts": {
    "build:ui": "pnpm -C packages/ui run build",
    "build:utils": "pnpm -C packages/utils run build",
    "build": "turbo run build",
    "dev:web": "concurrently --kill-others \"pnpm -C run dev packages/web\" \"pnpm -C run watch packages/ui\" \"pnpm  -C run watch packages/utils\"",
    "version": "bump"
  },
  "bump": {
    "changelog": true,
    "publish": false,
    "allowed_branches": [
      "master",
      "main"
    ],
    "mode": "monorepo",
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@innei/bump-version": "latest",
    "@innei/eslint-config-react-ts": "latest",
    "@innei/prettier": "latest",
    "@types/lodash-es": "4.17.6",
    "@types/node": "17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "lodash-es": "4.17.21",
    "typescript": "4.9.4",
    "vite": "^3",
    "vite-tsconfig-paths": "^3.4.0",
    "concurrently": "7.0.0",
    "turbo": "1.3.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
